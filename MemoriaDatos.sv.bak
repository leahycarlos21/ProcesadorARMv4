module MemoriaDatos #(parameter N=256)(input clk, WE3, 
									input logic [31:0] A,WD,
									output logic [31:0] RD);

logic [31:0] mem [0:N-1];
									
parameter Data = "ROM.txt";


									
									
always_ff @(posedge clk) 
begin
	if(WE3 & !clk) 
		begin
			initial begin
			$writememh(Data,WD,A + 8'h00000020);
			end
		end
	else 
		begin
			initial begin
			$readmemh(Data,mem);
			end

			assign RD = mem[A];
		end
	
	
end

									

endmodule 